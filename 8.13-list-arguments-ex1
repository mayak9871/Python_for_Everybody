# write a function called chop that takes a list and modifies it,
# removing the first and last elements

def chop(numbers):
    first_element = numbers.pop(0)
    last_element = numbers.pop(-1)
    print(first_element)
    print(last_element)

#assign values to numbers
numbers = [1,2,3,4,5,6]
numbers1 = [10,20,30,40]
#call the function
chop(numbers)
chop(numbers1)

# write a function called middle that takes a list
# and returns a new list that contains all but the first and last elements
def middle(num):
    print(num[1:-1])

num = [1,2,3,4,5,6]
num1 = [10,20,30,40]
middle(num)
middle(num1)
